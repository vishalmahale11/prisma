generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @id @default(autoincrement())
  email      String  @unique
  first_name String?
  last_name  String?
  password   String
  country    String
  todo       Todo[]
}

model Todo {
  id        Int    @id @default(autoincrement())
  task_name String
  duration  String
  user_id   Int
  user      User   @relation(fields: [user_id], references: [id])
}
